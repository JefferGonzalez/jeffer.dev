{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "isolatedModules": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@pages/*": ["src/pages/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
